{
  "response": {
    "contentType": "application/json"
  },
  "api": {
    "outbound": {
      "url": "https://whatsapp.praekelt.org/v1/messages"
    }
  },
  "users": {
    "url": "http://localhost:8080/v2/api/whatsapp/user",
    "path": {
      "id": "/id"
    }
  },
  "conversation": {
    "cutoffHours": "1",
    "startWordRegex": "/start/i"
  },
  "nlu": {
    "url": "http://learning-staging.eu-west-1.elasticbeanstalk.com/parse"
  },
  "core": {
    "url": {
      "base": "http://localhost:5000/",
      "suffix": "/parse"
    }
  },
  "auth": {
    "platform": "insert_platform_token_here"
  },
  "tasks": {
    "url": "http://localhost:8080/v2/api/task",
    "path": {
      "respond": "/respond",
      "create": "/create"
    }
  },
  "groups": {
    "url": "http://localhost:8080/v2/api/group",
    "path": {
      "list": "/fetch/minimal"
    }
  }
}
